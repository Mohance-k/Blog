component output = false{	// default "index"	public function index( event, rc, prc ) {		if (structKeyExists(rc,'username')){			writeDump(rc);			abort;		}	}	/* --------------------- PRIVATE EVENTS ----------------------------- */	public function onRequestStart( event, rc, prc ) {		// List of pages, can be accessed without logged in.		if (structKeyExists(RC,'fwreinit')){			Application.LoginService = createObject("component", "models.CFC.tblLoginUsersDAO");		}		local.WhiteList = ["Main.Index", "Security.Login", "User.Signup", "Main.Features", "Main.About", "Main.FAQ", "Main.Contact", "Profile.Search"];		if( NOT structKeyExists(Session, "UserID") AND NOT ArrayFindNoCase(Local.WhiteList, rc.Action)){			setNextEvent('Security.Login');		}	}}